#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 3;
   vector<int> userVals(NUM_ELEMENTS);
   unsigned int i;
   
   userVals.at(0) = 1;
   userVals.at(1) = 8;
   userVals.at(2) = 6;
   
   for (i = 0; i < userVals.size(); ++i) {
      cout << userVals.at(i) << endl;
   }
   
   return 0;
}


#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 3;
   vector<int> userVals(NUM_ELEMENTS);
   unsigned int i;
   int sumVal;
   
   userVals.at(0) = 1;
   userVals.at(1) = 9;
   userVals.at(2) = 5;

   sumVal = 0;
   for (i = 0; i < userVals.size(); ++i) {
      sumVal = sumVal + userVals.at(i);
      cout << sumVal << endl;
   }
   
   return 0;
}

#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 3;
   vector<int> userVals(NUM_ELEMENTS);
   int i;
   
   userVals.at(0) = 2;
   userVals.at(1) = 6;
   userVals.at(2) = 9;
   
   for (i = userVals.size() - 1; i >= 0; --i) {
      cout << userVals.at(i) << endl;
   }
   
   return 0;
}	

#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 4;
   vector<int> userVals(NUM_ELEMENTS);
   unsigned int i;
   int maxVal;
   
   userVals.at(0) = 2;
   userVals.at(1) = 6;
   userVals.at(2) = 8;
   userVals.at(3) = 4;
   
   maxVal = userVals.at(0);
   for (i = 0; i < userVals.size(); ++i) {
      if (userVals.at(i) >= maxVal) {
         maxVal = userVals.at(i);
         cout << maxVal << endl;
      }
   }
   
   return 0;
}


#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 4;
   vector<int> userVals(NUM_ELEMENTS);
   unsigned int i;
   
   userVals.at(0) = -2;
   userVals.at(1) = 3;
   userVals.at(2) = 5;
   userVals.at(3) = -7;
   
   for (i = 0; i < userVals.size(); ++i) {
      if (userVals.at(i) < 0) {
         cout << userVals.at(i) << endl;
      }
   }
   
   return 0;
}

#include <iostream>
#include <vector>
using namespace std;

int main() {
   const int NUM_ELEMENTS = 4;
   vector<int> userVals(NUM_ELEMENTS);
   unsigned int i;
   
   userVals.at(0) = -2;
   userVals.at(1) = 4;
   userVals.at(2) = -6;
   userVals.at(3) = 8;
   
   for (i = 0; i < userVals.size(); ++i) {
      if (userVals.at(i) < 0) {
         userVals.at(i) = -1 * userVals.at(i);
      }
      cout << userVals.at(i) << endl;
   }
   
   return 0;
}
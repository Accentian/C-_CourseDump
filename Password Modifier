#include <iostream>
#include <string>
using namespace std;

int main() {
   
   /* Type your code here. */
   string userPass;
   int i = 0;
   
   cin >> userPass;
   
   for (i = 0; i < userPass.size(); i++) {
      char currentChar = userPass.at(i);
      
      if (currentChar == 'i'){
         cout << "!";
      }
      else if (currentChar == 'a'){
         cout << "@";
      }
      else if (currentChar == 'm'){
         cout << "M";
      }
      else if (currentChar == 'B'){
         cout << "8";
      }
      else if (currentChar == 'o'){
         cout << ".";
      }
      else {
         cout << currentChar;
      }
   } // End For Loop
   
   cout << "q*s" << endl;

   return 0;
}
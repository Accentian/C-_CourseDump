#include <iostream>
using namespace std;

/* Your solution goes here  */
void PrintFeetInchShort(int feet, int inches){
    cout << feet << "' " << inches << "\"\n";
}

int main() {
   int userFeet;
   int userInches;

   cin >> userFeet;
   cin >> userInches;

   PrintFeetInchShort(userFeet, userInches);  // Will be run with (5, 8), then (4, 11)

   return 0;
}
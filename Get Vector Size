#include <iostream>
#include <vector>
using namespace std;

int main() {
   vector<int> sensorReadings(4);
   int currentSize;
   int input;
   cin >> input;

   sensorReadings.resize(input);

   /* Your solution goes here  */
   currentSize = 0;
   
   for (int i = 0; i < sensorReadings.size(); i++) {
      currentSize = currentSize + 1;
   }

   cout << "Number of elements: " << currentSize << endl;

   return 0;
}
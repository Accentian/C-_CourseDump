#include <iostream>
#include <string>
using namespace std;

int main() {
   string simonPattern;
   string userPattern;
   int userScore;
   int i;

   userScore = 0;

   cin >> simonPattern;
   cin >> userPattern;

   /* Your solution goes here  */
   for (i = 0; i < simonPattern.size(); i++) {
      if(simonPattern.at(i) == userPattern.at(i)){
         userScore++;
      }
      
      else{
         break;
      }
      
   }

   cout << "userScore: " << userScore << endl;

   return 0;
}